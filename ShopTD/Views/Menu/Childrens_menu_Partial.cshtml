@using ShopTD.Models
@model List<Menu>

@foreach (var item in Model)
{
    <li class="nav-item border border-info">
        <a href="#" class="nav-link">@item.Name</a>
        <ul class="nav menu">
            @Html.Action("Childrens_menu_Partial", "Menu", new { parent_id = item.Menu_ID })
        </ul>
    </li>
}


